<?xml version="1.0"?>
<extension key="circle_cloud_webhook" type="module">
  <file>circle_cloud_webhook</file>
  <name>Circle Cloud Webhook</name>
  <description>Adds webhooks to notify a 3rd party of updates to contacts</description>
  <license>AGPL-3.0</license>
  <authors>
    <author>
      <name>circle.cloud</name>
      <email>support@circle.cloud</email>
      <role>Maintainer</role>
    </author>
    <author>
      <name>Circle Interactive</name>
      <email>support@circle-interactive.co.uk</email>
    </author>
  </authors>
  <urls>
    <url desc="Documentation">https://integrations.circlecloud.tech/civicrm</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-06-24</releaseDate>
  <version>1.0.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.78</ver>
  </compatibility>
  <comments>Module for sending webhooks</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/CircleCloudWebhook</namespace>
    <format>25.01.1</format>
    <angularModule>crmCircleCloudWebhook</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
